{
    "name": "andrew72ru/yii2-mongodb-user",
    "description": "User management for Yii2 with mongodb",
    "keywords": ["yii2", "yii2-user", "user management", "mongodb"],
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "source": "https://github.com/andrew72ru/yii2-user"
    },
    "authors": [
        {
            "name": "Andrew Zhdanovskih",
            "email": "andrew72ru@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0.0",
        "yiisoft/yii2": "^2.0.9",
        "yiisoft/yii2-mongodb": "~2.1.0",
        "yiisoft/yii2-swiftmailer": "^2.0.0",
        "yiisoft/yii2-authclient": "^2.1.0",
        "yiisoft/yii2-bootstrap": "^2.0.0"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "^2.0.0",
        "codeception/specify": "*",
        "codeception/verify": "*",
        "codeception/aspect-mock": "*"
    },
    "autoload": {
        "psr-4": {
            "andrew72ru\\user\\": ""
        }
    },
    "extra": {
        "bootstrap": "andrew72ru\\user\\Bootstrap",
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
